docker commands:

    sudo docker build -t color-io-image .
    sudo docker run -dit --name color-io-container color-io-image /bin/sh
    sudo docker exec -it color-io-container /bin/sh

    sudo docker stop color-io-container
    sudo docker rm color-io-container

    sudo docker ps
    sudo docker system df
    sudo docker system prune

docker compose commands:

    sudo docker build -t color-io-image .

    sudo docker compose up -d

    sudo docker exec -it color-io-container /bin/sh

    sudo docker compose down
